{
  "name": "lacona-search-internet",
  "version": "0.1.1",
  "description": "Lacona App command to search the internet",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p lib; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laconalabs/lacona-search-internet.git"
  },
  "keywords": [
    "lacona",
    "command",
    "search",
    "internet",
    "web",
    "google",
    "bing",
    "yahoo",
    "amazon"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laconalabs/lacona-search-internet/issues"
  },
  "homepage": "https://github.com/laconalabs/lacona-search-internet",
  "dependencies": {
    "elliptical": "^1.0.0-rc10",
    "elliptical-string": "^1.0.0-rc2",
    "lacona-api": "0.1.1",
    "lacona-command": "^0.2.1",
    "lodash": "^4.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
