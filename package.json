{
  "name": "lacona-search-internet",
  "version": "0.2.2",
  "description": "Lacona App command to search the internet",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf build",
    "prepublish": "npm run clean && npm run build"
  },
  "lacona": {
    "config": "config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laconalabs/lacona-search-internet.git"
  },
  "keywords": [
    "lacona",
    "command",
    "search",
    "internet",
    "web",
    "google",
    "bing",
    "yahoo",
    "amazon"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laconalabs/lacona-search-internet/issues"
  },
  "homepage": "https://github.com/laconalabs/lacona-search-internet",
  "dependencies": {
    "elliptical": "^1.0.0-rc16",
    "lacona-api": "^0.6.2",
    "lacona-phrases": "^0.8.0",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
